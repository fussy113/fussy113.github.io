{
  "name": "fussy113.github.io",
  "packageManager": "yarn@4.8.1",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "engines": {
    "npm": "use yarn instead",
    "yarn": "4.8.1"
  },
  "volta": {
    "node": "22.14.0",
    "yarn": "4.8.1"
  },
  "scripts": {
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "dev": "vite",
    "format": "prettier --write src/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "markuplint": "markuplint  \"src/**/*.vue\"",
    "markuplint:fix": "markuplint --fix \"src/**/*.vue\"",
    "preview": "vite preview",
    "stylelint:fix": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "stylelint": "stylelint \"src/**/*.{vue,scss}\"",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@markuplint/vue-parser": "^4.6.18",
    "@markuplint/vue-spec": "^4.5.18",
    "@rushstack/eslint-patch": "^1.10.0",
    "@tailwindcss/vite": "^4.0.17",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.15",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.0.0",
    "markuplint": "^4.11.8",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "sass": "^1.86.1",
    "stylelint": "^16.20.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.8"
  }
}
