{
  "name": "fussy113.github.io",
  "packageManager": "yarn@4.2.2",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "engines": {
    "npm": "use yarn instead",
    "yarn": "4.2.2"
  },
  "volta": {
    "node": "20.13.1",
    "yarn": "4.2.2"
  },
  "scripts": {
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "dev": "vite",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "markuplint": "markuplint  \"src/**/*.vue\"",
    "markuplint:fix": "markuplint --fix \"src/**/*.vue\"",
    "preview": "vite preview",
    "stylelint:fix": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "stylelint": "stylelint \"src/**/*.{vue,scss}\"",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "vue": "^3.4.27",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@markuplint/vue-parser": "^4.2.0",
    "@markuplint/vue-spec": "^4.3.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "markuplint": "^4.9.1",
    "npm-run-all2": "^6.2.0",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.7"
  }
}
